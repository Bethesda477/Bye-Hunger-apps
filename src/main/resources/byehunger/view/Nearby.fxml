<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="700.0" prefWidth="1020.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="byehunger.view.NearbyController">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <top>
        <HBox spacing="5.0">
            <children>
                <TextField fx:id="locationField" promptText="Enter URL" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#handleGoButton" text="Go" />
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" />
            </BorderPane.margin>
        </HBox>
    </top>
    <center>
        <WebView fx:id="webView" />
    </center>
</BorderPane>
